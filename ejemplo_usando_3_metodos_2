public class  ejemplo_usando_3_metodos_2{
public static void main(String [] args){
int resultadomult; 
resultadomult = multiplicar(8,6);
System.out.println("El resultado de la multiplicacion es: "+resultadomult);

int resultadodiv;
resultadodiv = dividir(30,3);
System.out.println("El resultado de la division es: "+resultadodiv);	

int resultadosum;
resultadosum = sumar(6,6);
System.out.println("El resultado de la suma es: "+resultadosum);

int resultadorest;
resultadorest = restar(10,5);
System.out.println("El resultado de la resta es: "+resultadorest);

}
	public static int multiplicar(int num1, int num2){
	int multiplicacion = num1*num2;
	return multiplicacion;	
}
	public static int dividir(int num1, int num2){
	int division = num1/num2;
	return division;
}
	public static int sumar (int num1, int num2){
	int suma = num1+num2;
	return suma;
}
	public static int restar (int num1, int num2){
	int resta = num1+num2;
	return resta;
}
			
}
